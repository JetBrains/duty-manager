{
  "name": "duty-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn relay && next build",
    "start": "next start",
    "relay": "relay-compiler --schema main-schema/schema.graphql --src ./ --exclude '**/fauna/**' '**/fauna-src/**' '**/api/**' '**/.next/**' '**/node_modules/**' '**/test/**' '**/__generated__/**' '**/schema/**'",
    "relay-dev": "yarn relay --watch"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@jetbrains/ring-ui": "^2.1.16",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.2",
    "babel-plugin-relay": "^8.0.0",
    "core-js": "^2.6.11",
    "dotenv": "^8.2.0",
    "express-graphql": "^0.9.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-relay": "^0.6.0",
    "jsdom": "^16.0.0",
    "next": "9.2.0",
    "react": "^0.0.0-experimental-f42431abe",
    "react-dom": "^0.0.0-experimental-f42431abe",
    "react-relay": "^0.0.0-experimental-5f1cb628",
    "relay-compiler": "^8.0.0",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  },
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn"
    },
    "parser": "babel-eslint"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false
  }
}
